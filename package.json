{
  "name": "ymaps-distance",
  "version": "0.0.0",
  "homepage": "https://novakand.github.io/ymaps-distance/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href / --serve-path /",
    "build": "ng build",
    "build:gh": "ng build --configuration=production",
    "deploy": "npm run build:gh && npx ngh --dir=dist/ymaps-distance",
    "watch": "ng build --watch --configuration development",
    "fetch:polygons": "ts-node -P tsconfig.scripts.json scripts/fetch-polygons.ts",
    "build:index": "ts-node -P tsconfig.scripts.json scripts/build-index.ts",
    "fetch:all": "npm run fetch:polygons && npm run build:index",
    "dump:routes": "ts-node -P tsconfig.scripts.json scripts/dump-routes.ts",
    "routes:from:xlsx": "ts-node -P tsconfig.scripts.json scripts/xlsx-to-routes.ts",
    "routes:build": "npm run routes:from:xlsx && npm run dump:routes",
    "routes:merge": "ts-node -P tsconfig.scripts.json scripts/merge-excel.ts",
    "enrich:cities": "ts-node -P tsconfig.scripts.json scripts/enrich-cities-with-bbox.ts",
    "make:retry-list": "ts-node -P tsconfig.scripts.json scripts/make-retry-list.ts",
    "merge:routes": "ts-node -P tsconfig.scripts.json scripts/merge-route-lists.ts",
    "make:routes-list": "ts-node -P tsconfig.scripts.json scripts/make-routes-list.ts",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@deck.gl/geo-layers": "^9.1.12",
    "@deck.gl/layers": "^9.1.12",
    "@deck.gl/mapbox": "^9.1.12",
    "@deck.gl/mesh-layers": "^9.1.12",
    "@luma.gl/core": "^9.1.9",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@primeng/themes": "^19.1.3",
    "@turf/helpers": "^7.2.0",
    "@turf/turf": "^7.2.0",
    "@yandex/ymaps3-clusterer": "^0.0.10",
    "angular-yandex-maps-v3": "^19.0.0",
    "angular8-yandex-maps": "^19.0.0",
    "material-icons": "^1.13.14",
    "ngx-mapbox-gl": "^12.0.0",
    "primeicons": "^7.0.0",
    "primelocale": "^2.1.2",
    "primeng": "^19.1.3",
    "puppeteer": "^24.16.1",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.5",
    "@angular/cli": "^19.0.5",
    "@angular/compiler-cli": "^19.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/geojson": "^7946.0.16",
    "@types/jasmine": "~5.1.0",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^24.3.0",
    "@types/puppeteer": "^5.4.7",
    "@yandex/ymaps3-types": "^1.0.17179206",
    "angular-cli-ghpages": "^2.0.3",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.4",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4.1.8",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2"
  }
}
